{
  "name": "@skyalert/dynamodb",
  "description": "SkyAlert DynamoDB",
  "version": "0.1.1",
  "author": "Adrián Juárez <comecacahuates@yahoo.com>",
  "packageManager": "yarn@3.6.3",
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "format": "prettier --write .",
    "clean": "rm -rf dist",
    "compile": "tsc -p tsconfig.build.json && babel --config-file ./babel.config.build.cjs ./dist/esm --out-dir ./dist/cjs --out-file-extension .cjs --source-maps",
    "build": "yarn clean && yarn compile"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.7.0",
    "@shelf/jest-dynamodb": "^3.4.2",
    "@types/node": "^20.7.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "aws-sdk-client-mock": "^3.0.0",
    "aws-sdk-client-mock-jest": "^3.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "esbuild": "^0.19.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.521.0",
    "@aws-sdk/util-dynamodb": "3.521.0",
    "@stdlib/assert-is-object": "0.2.1",
    "@stdlib/random": "0.1.1",
    "@stdlib/utils-copy": "^0.1.0",
    "@stdlib/utils-entries": "0.2.0",
    "@stdlib/utils-from-entries": "0.2.1",
    "@stdlib/utils-merge": "^0.1.0",
    "@stdlib/utils-zip": "^0.2.1",
    "just-clone": "^6.2.0",
    "just-memoize": "^2.2.0",
    "just-typeof": "^3.2.0",
    "ts-pattern": "^5.0.5",
    "zod": "^3.22.2"
  }
}

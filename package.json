{
  "name": "@skyalert/dynamodb",
  "description": "SkyAlert DynamoDB",
  "version": "0.1.1",
  "author": "Adrián Juárez <comecacahuates@yahoo.com>",
  "packageManager": "yarn@3.6.3",
  "type": "module",
  "main": "dist/esm/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.js",
      "types": "./types/index.d.ts"
    }
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint",
    "format": "prettier --write .",
    "clean": "rm -rf dist types",
    "compile": "tsc -p tsconfig.build.json && babel --config-file ./babel.config.build.cjs ./dist/esm --out-dir ./dist/cjs --out-file-extension .cjs --source-maps",
    "build": "yarn clean && yarn compile"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.7.0",
    "@types/node": "^20.7.2",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "babel-jest": "^29.7.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "esbuild": "^0.19.4",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.421.0",
    "zod": "^3.22.2"
  }
}
